<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="2023-01-17-create-sec-user-table-import-1" author="emelianov-aa">
        <createTable tableName="sec_user">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="create_ts" type="timestamp">
                <constraints nullable="false"/>
            </column>
            <column name="archive_ts" type="timestamp"/>
            <column name="login" type="varchar" remarks="Логин">
                <constraints nullable="false"/>
            </column>
            <column name="password" type="varchar" remarks="Пароль">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="sec_user"/>
        </rollback>
    </changeSet>
</databaseChangeLog>